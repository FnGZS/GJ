}
.ping_cc {
  display: inline-block;
  font-size: 20rpx;
  color: #999;
  margin: 6rpx 0;
  margin-left: 5rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ping_dd{
  display: inline-block;
  font-size: 20rpx;
  color: #999;
  margin: 6rpx 0;
  overflow: hidden;
  text-overflow: ellipsis;
