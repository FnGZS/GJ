/* 商城 */
.ping {
  display: flex;
  width: 100%;
  padding: 5rpx 5rpx 0rpx 5rpx;
  border-bottom: 1rpx solid #f4f4f4;
}

.ping_b {
  flex: 0.25;
  text-align: center;
}

.ping image {
  width: 150rpx;
