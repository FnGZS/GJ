.icon-jingxuan:before { content: "\e609"; }

.icon-gerenjianjie:before { content: "\e672"; }

.teamDetail {
  width: 100%;
  height: 100%;
}

.headimg {
  width: 100%;
}

.headimg image {
  display: block;
  margin: auto;
  margin-top: 30rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.name-work {
  width: 70%;
  margin-left: 15%;
  height: 50rpx;
}

.name-work-main {
  margin-top: 10rpx;
  display: flex;
  /* text-align: center; */
  margin:auto;
  justify-content: center;
}
.name {
  display: block;
  font-size: 30rpx;
  color: #5c5c5c;
  line-height: 50rpx;
}

.work {
  display: block;
  font-size: 25rpx;
  line-height: 55rpx;
  margin-left: 10rpx;
  color: #5c5c5c;
}

.goal {
  width: 100%;
  height: 120rpx;
  display: flex;
  border-bottom: 1rpx solid #f1f1f1;
  /* background: red */
}

.score {
  width: 33.3%;
  text-align: center;
  color: #5c5c5c;
}

.score-num {
  font-size: 30rpx;
  margin-top: 25rpx;
}

.score-text {
  margin-top: 5rpx;
  font-size: 28rpx;
}

.yuyueNum {
  width: 33.3%;
  text-align: center;
  color: #5c5c5c;
}

.yuyueNum-num {
  font-size: 30rpx;
  margin-top: 25rpx;
}

.yuyueNum-text {
  margin-top: 5rpx;
  font-size: 28rpx;
}

.caseNum {
  width: 33.3%;
  text-align: center;
  color: #5c5c5c;
}

.caseNum-num {
  font-size: 30rpx;
  margin-top: 25rpx;
}

.caseNum-text {
  margin-top: 5rpx;
  font-size: 28rpx;
}

.coumLine {
  width: 1rpx;
  height: 50rpx;
  margin-top: 35rpx;
  background: #e1e1e1;
}

.intro-title{
  display: flex;
  color: #5c5c5c;
  margin-top: 40rpx;
  margin-left: 5%;
  padding-bottom: 20rpx;
  /* border-bottom: 1px solid #f1f1f1; */
}
.intro-title-line{
  width: 5rpx;
  height: 40rpx;
  background: #5c5c5c;
}
.icon-gerenjianjie{
  font-size: 40rpx;
}
.intro-title-text{
  line-height: 40rpx;
